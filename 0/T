FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      T;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 1 0 0 0];

internalField   uniform 298;

boundaryField
{
    front
    {
        type            wedge;
    }
    back
    {
        type            wedge;
    }
    inlet
    {
        type           totalTemperature; //totalTemperature
        value		uniform 3650;
        gamma		1.4;
        T0		uniform 3650;
        psi		thermo:psi;
        U		U;          
    }
    nozzle-wall
    {
        type            zeroGradient;
    }
    left-wall
    {
        type            zeroGradient;
    }
    horz-outlet
    {    
        type            zeroGradient;
    }
    vert-outlet
    {    
        type            zeroGradient;
    }
    


}


