FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0); // ()

boundaryField
{
    front
    {
        type            wedge;
    }
    
    back
    {
        type            wedge;
    }
    
    inlet
    {
        type           pressureInletOutletVelocity; //pressureInletOutletVelocity
        value		uniform ( 0 0 0 );
	//tangentialVelocity uniform (0 0 0);
    	//type		pressureInletOutletVelocity;
    	//value		uniform ( 1213 0 0 );
    }
    left-wall
    {
    
        type           slip;

    }
    nozzle-wall
    {
    
        type           slip;

    }
    horz-outlet
    {	/*
       type		inletOutlet;
       value		(0 0 0);
       inletValue	(0 0 0);
	*/
	
	//type		zeroGradient;
	
        type           pressureInletOutletVelocity; //pressureInletOutletVelocity
        value		uniform ( 0 0 0 );

	
    }
    
    vert-outlet
    {
	   type	pressureInletOutletVelocity;
        value		uniform ( 0 0 0 );	   
    }
    


}


