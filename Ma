{
    inlet
    {
        type            calculated;
        value           uniform 0;
    }
    vert-outlet
    {
        type            calculated;
        value           uniform 0;
    }
    front
    {
        type            wedge;
    }
    nozzle-wall
    {
        type            calculated;
        value           uniform 0;
    }
    back
    {
        type            wedge;
    }
    horz-outlet
    {
        type            calculated;
        value           uniform 0;
    }
    left-wall
    {
        type            calculated;
        value           uniform 0;
    }
